{
  "name": "default-node-proj-template",
  "version": "1.0.3",
  "main": "server.babel.js",
  "repository": "https://github.com/Twiage/default-node-proj-template.git",
  "author": "Twiage T3",
  "license": "Unlicensed",
  "private": true,
  "scripts": {
    "start": "node server.babel.js",
    "dev": "ACTIVE_PROFILE=development node -r dotenv/config server.babel.js",
    "lint": "eslint --quiet ./src",
    "lint:fix": "eslint --fix ./src",
    "test:unit": "cp .env.example .env && jest --config jest-config.json",
    "test:unit:watch": "jest --config jest-config.json --watch",
    "test:system": "cp .env.example .env && ava",
    "deploy:major": "yarn version --major && git push --follow-tags",
    "deploy:minor": "yarn version --minor && git push --follow-tags",
    "deploy:patch": "yarn version --patch && git push --follow-tags"
  },
  "devDependencies": {
    "ava": "*",
    "babel-eslint": "*",
    "babel-jest": "*",
    "chai": "*",
    "chai-sinon": "*",
    "dotenv": "*",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-lodash-fp": "*",
    "jest": "*",
    "mock-require": "*",
    "node-mocks-http": "*",
    "sinon": "*",
    "supertest": "*"
  },
  "ava": {
    "files": [
      "**/__system-tests__/*.js"
    ],
    "sources": [
      "**/*.js",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "powerAssert": false,
    "require": [
      "@babel/register",
      "@babel/polyfill",
      "dotenv/config"
    ],
    "babel": {
      "testOptions": {
        "babelrc": false,
        "configFile": false
      }
    }
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "acl": "*",
    "bluebird": "*",
    "body-parser": "*",
    "compression": "*",
    "config": "*",
    "cors": "*",
    "crypto": "*",
    "dd-trace": "^0.16.0",
    "express": "*",
    "express-winston": "*",
    "generate-password": "*",
    "glob": "*",
    "helmet": "*",
    "http-errors": "*",
    "http-status-codes": "*",
    "is-cidr": "*",
    "jsonwebtoken": "*",
    "lodash": "*",
    "moment": "*",
    "mongodb": "*",
    "mongoose": "~5.7.7",
    "mongoose-unique-validator": "*",
    "os": "*",
    "owasp-password-strength-test": "*",
    "passport": "*",
    "passport-jwt": "*",
    "passport-saml": "*",
    "request": "*",
    "validator": "*",
    "winston": "*",
    "xml2js": "0.4.22"
  }
}
